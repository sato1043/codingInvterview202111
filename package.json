{
  "name": "codingInvterview202111",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "run-p next:dev pathpida:watch",
    "next:dev": "next dev",
    "pathpida": "pathpida",
    "pathpida:watch": "pathpida --watch",
    "pre:build": "rimraf .next out && pathpida",
    "pre:push": "run-s pre:build pathpida prettier tslint eslint test",
    "prettier": "prettier --list-different --write .",
    "tslint": "tsc -p . --noEmit",
    "eslint": "eslint . --fix",
    "test": "jest",
    "build": "npm run pre:build && next build && next export"
  },
  "dependencies": {
    "@emotion/cache": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/noto-sans-jp": "^4.5.2",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@types/validator": "^13.6.6",
    "axios": "^0.21.4",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "next": "^12.0.3",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.19.1",
    "sass": "^1.35.1",
    "the-new-css-reset": "^1.3.1",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.5.0",
    "@next/eslint-plugin-next": "^12.0.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.176",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/typescript-estree": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-mock-axios": "^4.4.1",
    "next-router-mock": "^0.6.2",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.15.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "typescript-eslint-language-service": "^4.1.5"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "src/utils/$path.ts",
      "out"
    ],
    "env": {
      "browser": true,
      "es2020": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "import",
      "unused-imports",
      "react",
      "react-hooks",
      "jsx-a11y",
      "@emotion",
      "@next/next"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:jsx-a11y/recommended",
      "plugin:@next/next/recommended",
      "prettier"
    ],
    "settings": {
      "node": {
        "tryExtensions": [
          ".ts",
          ".tsx",
          ".js",
          ".jsx",
          ".json",
          ".node"
        ]
      },
      "import/resolver": {
        "node": {
          "extensions": [
            ".ts",
            ".tsx",
            ".js",
            ".jsx",
            ".json",
            ".node"
          ]
        }
      },
      "import/ignore": [
        "node_modules"
      ],
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "no-dupe-class-members": 0,
      "spaced-comment": [
        2,
        "always",
        {
          "line": {
            "markers": [
              "/"
            ]
          },
          "block": {
            "balanced": true
          }
        }
      ],
      "no-extra-boolean-cast": 0,
      "no-unused-vars": 0,
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "no-array-constructor": 0,
      "@typescript-eslint/no-array-constructor": 2,
      "@typescript-eslint/adjacent-overload-signatures": 2,
      "@typescript-eslint/no-namespace": [
        2,
        {
          "allowDeclarations": true
        }
      ],
      "@typescript-eslint/prefer-namespace-keyword": 2,
      "@typescript-eslint/no-var-requires": 2,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-module-boundary-types": 2,
      "react/jsx-no-target-blank": "error",
      "react/jsx-uses-react": [
        1
      ],
      "react/prop-types": 0,
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "sort-imports": 0,
      "import/order": [
        2,
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index",
            "object",
            "type"
          ],
          "pathGroups": [
            {
              "pattern": "@alias/**",
              "group": "parent",
              "position": "before"
            }
          ],
          "alphabetize": {
            "order": "asc"
          },
          "newlines-between": "always"
        }
      ],
      "unused-imports/no-unused-imports-ts": 2,
      "jsx-a11y/alt-text": 1,
      "jsx-a11y/aria-role": 1,
      "@emotion/jsx-import": 2,
      "@emotion/pkg-renaming": 2
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.js"
        ]
      }
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    },
    "globalSetup": "<rootDir>/__tests__/setupEnv.ts",
    "setupFiles": [
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup*.(js|tsx?)*",
      "<rootDir>/__tests__/fixtures.ts",
      "<rootDir>/__tests__/mocks/*.(js|tsx?)*"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(scss|css)$": "<rootDir>/__mocks__/styleMock.ts"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ]
  }
}
